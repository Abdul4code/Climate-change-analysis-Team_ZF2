   "source": [
    "nlp = spacy.load('en_core_web_sm')\n",
    "\n",
    "def entities(df):\n",
    "    df_index = 0\n",
    "\n",
    "    for tweet in df['message']:\n",
    "        tweet = nlp(tweet)\n",
    "\n",
    "    for entity in tweet.ents:\n",
    "        df.loc[df_index, 'message'] = df.loc[df_index, 'message'].replace(str(entity.text), str(entity.label_))\n",
    "        df_index += 1\n",
    "\n",
    "        return df"
   ]
